#!/bin/bash

if [ ! -d git_eleves_eval ]
then
    echo "Big pb, pas de git élèves"
fi

cd git_eleves_eval

if [ $1 -le 9 ]
then
    dir_eleve="rs2022proj20$1"
else
    dir_eleve="rs2022proj2$1"
fi

echo "Groupe $1"
cd $dir_eleve
make
#ls
echo "Test arguments"
./tests/test_args $1
echo "Test parcour"
./tests/test_parkour $1
echo "Test name"
./tests/test_name $1
echo "Test size"
./tests/test_size $1
echo "Test date"
./tests/test_date $1
echo "Test mime"
./tests/test_mime $1
echo "Test regex"
./tests/test_regex $1
echo "Test grep"
./tests/test_grep $1
echo "Test dir"
./tests/test_dir $1
echo "Test perm"
./tests/test_perm $1
cd ..


